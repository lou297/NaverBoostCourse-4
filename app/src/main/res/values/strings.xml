    <string name="movie4_credits">8.59</string>
    <string name="movie4_release">2017.01.25</string>
    <string name="movie4_genre">액션, SF, 스릴러</string>
    <string name="movie4_show_time">106분</string>
    <string name="movie4_rank">4</string>
